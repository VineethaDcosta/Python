a = int(input("Enter num1: "))
b = int(input("Enter num2: "))
c = int(input("Enter num3: "))
if( a >= b and a <= c ):
  print("The second largest number is ",a) # Here, b>a>c
elif( b >= a and b <= c ):
  print("The second largest number is ",b) # Here, a>b>c
else:
  print("The second largest number is ",c)
